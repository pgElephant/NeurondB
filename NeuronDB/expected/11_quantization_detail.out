-- Extension created in 01_types_basic
SET neurondb.gpu_enabled = off;
-- INT8 quantization produces one byte per dimension
SELECT octet_length(vector_to_int8_gpu('[1,-1,0,3]'::vector));
 octet_length 
--------------
            4
(1 row)

-- FP16 quantization produces two bytes per dimension
SELECT octet_length(vector_to_fp16_gpu('[1,-1,0,3]'::vector));
 octet_length 
--------------
            8
(1 row)

-- Binary quantization packs bits; 4 dims => 1 byte
SELECT octet_length(vector_to_binary_gpu('[1,-1,0,3]'::vector));
 octet_length 
--------------
            1
(1 row)

